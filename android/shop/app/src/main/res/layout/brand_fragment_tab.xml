<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bottom_border">

    <View
        android:id="@+id/view104"
        android:layout_width="0.5dp"
        android:layout_height="0.5dp"
        app:layout_constraintEnd_toStartOf="@+id/view101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view101"
        android:layout_width="0.5dp"
        android:layout_height="0.5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view103"
        android:layout_width="0.5dp"
        android:layout_height="0.5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view101"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView305"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:padding="8dp"
        android:text="@string/comprehensive"
        android:textAlignment="center"
        android:textColor="@color/colorFont_morandi"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/view104"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView306"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="@string/sales_volume"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView305"
        app:layout_constraintEnd_toStartOf="@+id/view101"
        app:layout_constraintStart_toEndOf="@+id/view104"
        app:layout_constraintTop_toTopOf="@+id/textView305" />

    <TextView
        android:id="@+id/textView307"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="@string/price"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView306"
        app:layout_constraintEnd_toStartOf="@+id/view103"
        app:layout_constraintStart_toEndOf="@+id/view101"
        app:layout_constraintTop_toTopOf="@+id/textView306" />

    <TextView
        android:id="@+id/textView308"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center_vertical"
        android:text="@string/new_product"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/textView307"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/view103"
        app:layout_constraintTop_toTopOf="@+id/textView307" />

    <ImageView
        android:id="@+id/imageView156"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:tag="down"
        app:layout_constraintBottom_toBottomOf="@+id/textView307"
        app:layout_constraintEnd_toStartOf="@+id/textView308"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="@+id/textView307"
        app:layout_constraintTop_toTopOf="@+id/textView307"
        app:srcCompat="@drawable/icon_up_down_gray" />

</android.support.constraint.ConstraintLayout>