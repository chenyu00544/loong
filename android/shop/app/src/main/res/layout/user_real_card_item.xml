<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/item_bg_selector">

    <View
        android:id="@+id/view80"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@color/gray_d"
        app:layout_constraintBottom_toBottomOf="@+id/textView143"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView143" />

    <TextView
        android:id="@+id/textView143"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="12dp"
        android:paddingBottom="8dp"
        android:paddingTop="8dp"
        android:text="@string/card"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView149"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="12dp"
        android:text="@string/reminder"
        android:textColor="@color/gray"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView143" />

    <ImageView
        android:id="@+id/imageView68"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:paddingEnd="8dp"
        android:paddingRight="8dp"
        android:scaleType="fitXY"
        android:paddingTop="10dp"
        app:layout_constraintEnd_toStartOf="@+id/view42"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView149"
        app:srcCompat="@drawable/icon_up_card"
        tools:ignore="RtlSymmetry" />

    <ImageView
        android:id="@+id/imageView69"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:paddingEnd="8dp"
        android:paddingRight="8dp"
        android:paddingTop="10dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="@+id/imageView68"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view42"
        app:layout_constraintTop_toTopOf="@+id/imageView68"
        app:srcCompat="@drawable/icon_up_card"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/textView150"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/examples"
        android:textColor="@color/gray"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView68"
        app:layout_constraintStart_toStartOf="@+id/imageView68"
        app:layout_constraintTop_toBottomOf="@+id/imageView68" />

    <TextView
        android:id="@+id/textView151"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/examples"
        android:textColor="@color/gray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView150"
        app:layout_constraintEnd_toEndOf="@+id/imageView69"
        app:layout_constraintStart_toStartOf="@+id/imageView69"
        app:layout_constraintTop_toTopOf="@+id/textView150" />

    <ImageView
        android:id="@+id/imageView70"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/card_back"
        app:layout_constraintBottom_toBottomOf="@+id/imageView71"
        app:layout_constraintEnd_toEndOf="@+id/imageView69"
        app:layout_constraintStart_toStartOf="@+id/imageView69"
        app:layout_constraintTop_toBottomOf="@+id/textView151"
        app:srcCompat="@drawable/card_back" />

    <ImageView
        android:id="@+id/imageView71"
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/card_front"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView68"
        app:layout_constraintStart_toStartOf="@+id/imageView68"
        app:layout_constraintTop_toBottomOf="@+id/textView150"
        app:srcCompat="@drawable/card_front" />

    <ImageView
        android:id="@+id/imageView72"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/icon_delete"
        app:layout_constraintBottom_toBottomOf="@+id/imageView73"
        app:layout_constraintEnd_toEndOf="@+id/imageView69"
        app:layout_constraintTop_toTopOf="@+id/imageView73"
        app:srcCompat="@drawable/icon_delete" />

    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/icon_delete"
        app:layout_constraintEnd_toEndOf="@+id/imageView68"
        app:layout_constraintTop_toTopOf="@+id/imageView68"
        app:srcCompat="@drawable/icon_delete" />

    <View
        android:id="@+id/view42"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.500"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView149" />
</android.support.constraint.ConstraintLayout>