<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:focusable="true"
    android:focusableInTouchMode="true">

    <View
        android:id="@+id/view52"
        android:layout_width="wrap_content"
        android:layout_height="7dp"
        android:background="@color/gray_f5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView272"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:padding="4dp"
        android:text="@string/ship_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view52" />

    <TextView
        android:id="@+id/textView273"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:padding="4dp"
        android:text="@string/ship_no"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/textView272"
        app:layout_constraintTop_toBottomOf="@+id/textView272" />

    <EditText
        android:id="@+id/editText22"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/shape_4_grad_b_white"
        android:ems="10"
        android:gravity="center_vertical"
        android:inputType="textPersonName"
        android:padding="4dp"
        android:text=""
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView272"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView272"
        app:layout_constraintTop_toTopOf="@+id/textView272" />

    <EditText
        android:id="@+id/editText23"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/shape_4_grad_b_white"
        android:ems="10"
        android:gravity="center_vertical"
        android:inputType="textPersonName"
        android:padding="4dp"
        android:text=""
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView273"
        app:layout_constraintEnd_toEndOf="@+id/editText22"
        app:layout_constraintStart_toEndOf="@+id/textView273"
        app:layout_constraintTop_toTopOf="@+id/textView273" />

</android.support.constraint.ConstraintLayout>