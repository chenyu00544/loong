<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView97"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_time" />

    <TextView
        android:id="@+id/textView228"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="4dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="4dp"
        android:text="@string/today"
        android:textColor="@color/gray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView97"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView97"
        app:layout_constraintTop_toTopOf="@+id/imageView97" />

    <View
        android:id="@+id/view69"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginBottom="4dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_d"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView228"
        app:layout_constraintStart_toStartOf="@+id/imageView97"
        app:layout_constraintTop_toBottomOf="@+id/imageView97" />

    <View
        android:id="@+id/view70"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/view69"
        app:layout_constraintEnd_toEndOf="@+id/textView228"
        app:layout_constraintStart_toEndOf="@+id/view69"
        app:layout_constraintTop_toBottomOf="@+id/textView228" />

    <ImageView
        android:id="@+id/imageView101"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView234"
        app:layout_constraintEnd_toEndOf="@+id/view71"
        app:layout_constraintTop_toTopOf="@+id/textView234"
        app:srcCompat="@drawable/icon_forward" />

    <TextView
        android:id="@+id/textView232"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="天外出散步"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/view70"
        app:layout_constraintStart_toStartOf="@+id/view70"
        app:layout_constraintTop_toTopOf="@+id/view70" />

    <ImageView
        android:id="@+id/imageView100"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@+id/textView232"
        app:layout_constraintTop_toBottomOf="@+id/textView232"
        app:srcCompat="@drawable/icon_no_pic" />

    <TextView
        android:id="@+id/textView233"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:gravity="center_vertical"
        android:text="天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步天外出散步"
        android:textColor="@color/gray"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView100"
        app:layout_constraintEnd_toEndOf="@+id/textView232"
        app:layout_constraintStart_toEndOf="@+id/imageView100"
        app:layout_constraintTop_toTopOf="@+id/imageView100" />

    <View
        android:id="@+id/view71"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="@color/gray_d"
        app:layout_constraintEnd_toEndOf="@+id/textView233"
        app:layout_constraintStart_toStartOf="@+id/imageView100"
        app:layout_constraintTop_toBottomOf="@+id/imageView100" />

    <TextView
        android:id="@+id/textView234"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/check_info"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view70"
        app:layout_constraintEnd_toStartOf="@+id/imageView101"
        app:layout_constraintStart_toStartOf="@+id/view71"
        app:layout_constraintTop_toBottomOf="@+id/view71" />

</android.support.constraint.ConstraintLayout>