<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dp" />
            <gradient
                android:angle="270"
                android:centerColor="@color/gray_f5"
                android:endColor="@color/gray_f5"
                android:startColor="@color/gray_f5" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2dp" />
                <gradient
                    android:centerColor="@color/colorBack_morandi"
                    android:endColor="@color/colorFont_morandi"
                    android:startColor="@color/colorFront_morandi" />
            </shape>
        </clip>
    </item>
</layer-list>
