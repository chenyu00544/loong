        <shape android:dither="true" android:shape="oval">
            <solid android:color="@color/line_long_dark" />
            <stroke android:width="1px" android:color="@color/view_color" />
        </shape>
    </item>
    <item android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp">
        <shape android:shape="oval">
            <solid android:color="@color/public_theme_color_normal" />
            <size android:height="20dp" android:width="20dp" />
        </shape>
    </item>
</layer-list>
