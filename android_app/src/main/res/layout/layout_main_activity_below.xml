<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/main_below_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/menu_bg"
        android:visibility="visible" />

    <com.ecjia.component.view.ECJiaScrollView_Main
        android:id="@+id/main_sv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="130dp"
        android:background="#00000000"
        android:fadingEdge="UNKNOWN_DATA_0x0"
        android:overScrollMode="never"
        android:scrollbars="UNKNOWN_DATA_0x0">

        <LinearLayout
            android:id="@+id/scorllview_ll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <View
            android:id="@+id/myfind_top_empty"
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x5a01" />

        <LinearLayout
            android:id="@+id/myfind_home_item"
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x2801"
            android:gravity="top|bottom|center_vertical|fill_vertical|center|fill"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="UNKNOWN_DATA_0x1701"
                android:layout_height="UNKNOWN_DATA_0x1701"
                android:layout_marginLeft="30dp"
                android:background="@drawable/profile_home" />

            <TextView
                android:layout_width="UNKNOWN_DATA_0x1"
                android:layout_height="wrap_content"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="20dp"
                android:layout_weight="1.0"
                android:text="@string/main_home"
                android:textColor="#ffffff"
                android:textSize="@dimen/list_item_title" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x4ccccd31"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:background="#30e8e8e8" />

        <LinearLayout
            android:id="@+id/left_first_group_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

        <View
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x4ccccd31"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:background="#30e8e8e8" />

        <LinearLayout
            android:id="@+id/left_second_group_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
        />
    </com.ecjia.component.view.ECJiaScrollView_Main>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="UNKNOWN_DATA_0x9601"
        android:orientation="vertical"
        android:paddingLeft="30dp"
        android:paddingRight="80dp">

        <View
            android:id="@+id/main_below_top"
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x1801"
            android:visibility="gone" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/profile_head"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <com.ecjia.component.imagecircle.ECJiaCircleImage
                    android:id="@+id/profile_newuser_img"
                    android:layout_width="UNKNOWN_DATA_0x4601"
                    android:layout_height="UNKNOWN_DATA_0x4601"
                    android:layout_marginTop="50dp"
                    android:adjustViewBounds="true"
                    android:src="@drawable/profile_no_avarta_icon" />

                <FrameLayout
                    android:id="@+id/profile_head_text"
                    android:layout_width="match_parent"
                    android:layout_height="UNKNOWN_DATA_0x4601"
                    android:layout_marginLeft="20dp"
                    android:layout_marginTop="50dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:minWidth="60dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/user_name"
                            android:layout_width="wrap_content"
                            android:layout_height="UNKNOWN_DATA_0x1"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1.0"
                            android:gravity="top|bottom|center_vertical|fill_vertical|center|fill"
                            android:textSize="17dp" />

                        <TextView
                            android:id="@+id/user_level"
                            android:layout_width="wrap_content"
                            android:layout_height="UNKNOWN_DATA_0x1"
                            android:layout_gravity="top|bottom|center_vertical|fill_vertical|center|fill"
                            android:layout_marginBottom="10dp"
                            android:layout_weight="1.0"
                            android:gravity="top|bottom|center_vertical|fill_vertical|center|fill"
                            android:textSize="@dimen/list_item_title" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/no_login"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|bottom|center_vertical|fill_vertical|center|fill"
                        android:text="@string/main_no_login"
                        android:textSize="17dp" />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/profile_setting_ll"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
                android:layout_marginLeft="180dp"
                android:layout_marginRight="10dp"
                android:gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="UNKNOWN_DATA_0x3001"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/profile_setting"
                        android:layout_width="UNKNOWN_DATA_0x1c01"
                        android:layout_height="UNKNOWN_DATA_0x1c01"
                        android:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end"
                        android:background="@drawable/icon_find_setting_black"
                        android:padding="5dp" />
                </FrameLayout>

                <TextView
                    android:id="@+id/profile_newset"
                    android:layout_width="wrap_content"
                    android:layout_height="UNKNOWN_DATA_0x1901"
                    android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end"
                    android:text="@string/setting"
                    android:textColor="#000000"
                    android:textSize="@dimen/list_item_title"
                    android:visibility="gone" />
            </LinearLayout>
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="UNKNOWN_DATA_0x1e01" />
    </LinearLayout>
</FrameLayout>
