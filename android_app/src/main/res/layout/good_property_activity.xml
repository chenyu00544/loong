<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@drawable/new_good_distance" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/top_view" />
    <ListView android:id="@+id/property_list" android:layout_width="match_parent" android:layout_height="wrap_content" android:divider="0x0" android:headerDividersEnabled="false" android:overScrollFooter="#00000000" />
    <FrameLayout android:orientation="vertical" android:id="@+id/no_info" android:background="#ffffff" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
        <TextView android:textSize="18dp" android:textColor="#999999" android:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="130dp" android:text="@string/property_no_info" />
        <ImageView android:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end" android:layout_width="wrap_content" android:layout_height="wrap_content" android:src="@drawable/searcher_no_result_empty_icon" />
    </FrameLayout>
</LinearLayout>
