<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/starta"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY" />

        <Button
            android:id="@+id/go_ecjia"
            android:layout_width="UNKNOWN_DATA_0x7801"
            android:layout_height="UNKNOWN_DATA_0x3201"
            android:layout_gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end"
            android:layout_marginBottom="20dp"
            android:background="@drawable/shape_start_viewpager"
            android:text="点击进入"
            android:textColor="#ffffffff"
            android:textSize="20dp"
            android:visibility="gone" />

        <Button
            android:id="@+id/welcome_intent1"
            android:layout_width="UNKNOWN_DATA_0x3c01"
            android:layout_height="UNKNOWN_DATA_0x1901"
            android:layout_gravity="left|right|center_horizontal|fill_horizontal|center|fill|start|end"
            android:layout_marginRight="15dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/shape_start_viewpager"
            android:gravity="top|bottom|left|right|center_vertical|fill_vertical|center_horizontal|fill_horizontal|center|fill|start|end"
            android:text="@string/skip"
            android:textColor="#ffffffff"
            android:textSize="15dp"
            android:visibility="visible" />
    </FrameLayout>
</LinearLayout>
