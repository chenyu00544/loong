.index-search-box {
  top: 1rpx;
  left: 0rpx;
  right: 0rpx;
  z-index: 999;
  position: fixed;
  background: #fff;
  padding: 13rpx;
  border-bottom: 1px solid #eee;
}

.index-search {
  border-bottom: 0;
  background: #fff;
  border-radius: 8rpx;
  overflow: hidden;
  position: relative;
  font-size: 32rpx;
  color: #999;
  box-sizing: border-box;
  height: 60rpx;
  line-height: 60rpx;
}

.cate-cont-box .cate-img image {
  border: 0;
}

.cate-content {
  background: #fff;
}

.cate-left{
  border-right:1rpx solid #ddd; 
}

.cate-right{
  height:95%;
}
.type-nav{
  border-right:0;
  color:#555;
}
.type-nav.selected{
  background:#fff;
  color:#000;
  border-left:0;
}