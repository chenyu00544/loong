.slide-image {
  width: 100%;
  height: 100%;
  margin: 0 auto;
  display: block;
  background: #fff;
}

/*banner*/

.banner-box {
  height: 750rpx;
  border-bottom: 1rpx solid #e4e4e4;
}

.user-orderJtou {
  color: #999;
  font-size: 26rpx;
  position: absolute;
}

.goods-fixed-icon text {
  color: #fff;
  font-size: 22rpx;
  text-align: center;
  display: block;
}

/*分享*/

.goods-sharing-icon {
  font-size: 42rpx;
  padding-left: 20rpx;
  color: #666;
}

/*优惠券*/

.goods-coupons-box .image {
  display: inline-block;
  width: 96rpx;
  height: 36rpx;
  position: absolute;
  top: 50%;
  margin-top: -21rpx;
}

.goods-coupons-box .image image {
  width: 100%;
  height: 100%;
  display: block;
  margin-top: 5rpx;
}

.goods-coupons-box .text {
  padding-left: 110rpx;
  color: #333;
}

.goods-title .title {
  border-right: 1px solid #eee;
  padding-right: 10rpx;
}

.common-title-box .l-title {
  width: 80rpx;
  display: block;
}

.goods-coupon-cont coupons-li .max-right.active {
  background: #ccc;
  color: #efefef;
}

/*玩法*/

.group-detail-cont {
  padding: 10rpx 0;
}

.group-detail-cont .num-left {
  position: absolute;
  left: 50%;
  z-index: 2;
}

.bargain-detail-cont .p {
  padding-top: 60rpx;
}

.bargain-detail-cont .n-list-xian, .bargain-detail-cont .n-list-xian-first,
.bargain-detail-cont .n-list-xian-last {
  top: 35%;
}

.group-detail-cont .n-list-xian-first {
  border-top: 1px solid #e7e8ef;
  left: 50%;
  top: 22%;
  z-index: 1;
  width: 60%;
}

.bargain-detail-cont .em {
  border: 1rpx dashed #ccc;
  color: #ccc;
  border-radius: 20rpx;
  position: absolute;
  left: 50%;
  margin-left: -20rpx;
  top: 15rpx;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  display: block;
  background: #fff;
}

.group-detail-cont .n-list-xian {
  border-top: 1px solid #e7e8ef;
  top: 35%;
  z-index: 1;
  width: 100%;
}

.group-detail-cont .n-list-xian-first {
  border-top: 1px solid #e7e8ef;
  left: 50%;
  top: 35%;
  z-index: 1;
  width: 60%;
}

.group-detail-cont .n-list-xian-last {
  border-top: 1px solid #e7e8ef;
  right: 50%;
  top: 35%;
  z-index: 1;
  width: 60%;
}

.bargain-play .more {
  padding-right: 30rpx;
}

.bargain-paly-mol .ul {
  padding: 15rpx 12px;
}

.bargain-paly-mol .ul .li {
  padding: 15rpx;
  font-size: 26rpx;
  color: #666;
}

/*成团列表*/

.group-wait-ul .title {
  padding: 0 0 20rpx 50rpx;
}

.group-wait-ul .li .cont {
  border: 1px solid #df2e26;
  border-radius: 90rpx;
  height: 90rpx;
}

.group-wait-ul .li .cont .img {
  width: 90rpx;
  height: 94rpx;
  overflow: hidden;
  border-radius: 90rpx;
  margin-right: 10rpx;
}

.group-wait-ul .li .cont .img image {
  width: 90rpx;
  height: 90rpx;
}

.group-wait-ul .li .cont .right .btn-wait {
  background: #df2e26;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  padding: 0 40rpx 0 15rpx;
  display: block;
  border-radius: 0 90rpx 90rpx 0;
}

.group-wait-ul .li .cont .img {
  width: 90rpx;
  height: 90rpx;
  overflow: hidden;
  border-radius: 90rpx;
  margin-right: 10rpx;
}

.group-wait-ul .li .cont .right .icon-xiangyoujiantou {
  position: absolute;
  top: 50%;
  color: #fff;
  right: 12rpx;
  margin-top: -12rpx;
  font-size: 26rpx;
}

.group-wait-ul .li {
  margin-bottom: 25rpx;
}

.goods-bargain-img {
  background: -webkit-linear-gradient(left bottom, #df2e26, #f96e52);
  background: -o-linear-gradient(left bottom, #df2e26, #f96e52);
  background: -mos-linear-gradient(left bottom, #df2e26, #f96e52);
  background: -moz-linear-gradient(left bottom, #df2e26, #f96e52);
  background: linear-gradient(left bottom, #df2e26, #f96e52);
  text-align: center;
  z-index: 10;
  color: #fff;
  font-size: 28rpx;
  padding: 14rpx 24rpx;
}

.goods-bargain-img .voteText {
  background: #fa2e46;
  border-radius: 4px;
  color: #fff;
  font-size: 26rpx;
  margin: 10rpx 0 10rpx;
  padding: 0 10rpx;
}

.goods-bargain-img .left .bargain-icon {
  border: 1px solid #fff;
  border-radius: 4px;
  margin-top: 12rpx;
}

.goods-bargain-img .left .icon-shehuituanti {
  background: #fff;
  color: #df2e26;
  padding: 7rpx 8rpx 0 8rpx;
  display: block;
}

.goods-bargain-img .left .bargain-num {
  padding: 0 8rpx;
}

.goods-bargain-img .left .pro-num {
  font-size: 38rpx;
  text-align: left;
  padding-left: 8rpx;
}

/*2018-2-22修改*/

.order-free {
  width: 100%;
  background-color: #df2e26;
  color: #fff;
  text-align: center;
  line-height: 46px;
}

.goods-bargain-img .left .icon-cuxiao {
  background: #fff;
  color: #df2e26;
  padding: 7rpx 8rpx 0 8rpx;
  display: block;
}

.index-fixed-box {
  height: 60rpx;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 30rpx;
  left: 15rpx;
  z-index: 30;
  color: #fff;
  line-height: 2.4rem;
  border-radius: 8rpx;
  overflow: hidden;
}

.index-fixed-imgbox {
  width: 65rpx;
}

.index-header-img {
  height: 60rpx;
  width: 60rpx;
  border-radius: 8rpx;
  overflow: hidden;
}

.index-header-img image {
  height: 60rpx;
  width: 60rpx;
}

.onelist-hidden {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.index-fixed-box .title {
  padding: 0 20rpx 0 10rpx;
  font-size: 26rpx;
  line-height: 60rpx;
  display: block;
}

.index-fixed-box .title text {
  color: #ccc;
}

.index-fixed-box .title text {
  color: #ccc;
}
