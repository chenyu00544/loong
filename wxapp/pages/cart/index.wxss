.image{ width: 100%;height: 100%}
.cart-tab-item-btn{width: 220rpx; display: block; height: 100rpx; line-height: 100rpx; font-size: 32rpx; background: #ff495e; color: #fff; float: right; text-align: center;}
.cart-tab-item{width: 375rpx;}
.flow-no-cart .gwc-bg{height:180rpx; width:180rpx; line-height:180rpx;  text-align: center; display: block; background:#DFE0E8; border-radius: 100%; margin: 0 auto; margin-top:10rpx}
 .flow-no-cart .icon-cart{font-size:100rpx; color:#fff;}
 .flow-no-cart p.t-remark{margin-top:1.6rem;}
 .flow-shop_name .cart-pro-select{margin:0 0 0 24rpx;line-height:138rpx;display:block;}
 .flow-goods-list .cart-pro-select{margin:9% 0 0 24rpx}
 .flow-footer .total-select{padding:26rpx 0 0 24rpx;}

.dish-item{width: 100%; padding: 26rpx 0; border-bottom:1rpx solid #eee; position:relative; background:#fff;}
.dish-item-wrp{width:100%; display:flex; flex-direction:row;}
.dish-item-pic{width:100%; margin-left:10px;}
.dish-item-pic image{width: 200rpx; height: 200rpx; margin: 0 auto; background:#fff; border:1rpx solid #eee;}
.dish-item-info{padding-left: 16rpx; margin-right:15px; width:68%;}
.dish-item-sales{color: #ccc; font-size:20rpx; line-height: 50rpx}
.dish-item-money{color: #ff495e; font-size: 32rpx; padding:0 0 10rpx 0;}
.flow-dete{position:absolute;bottom:10rpx;right:0rpx;}
.flow-dete .icon-lajixiang{color:#777;font-size:34rpx;}
.cart-tab-item{overflow:hidden;line-height:100rpx;}
.cart-tab-item .cart-item-total-price{float: left;}
.wx-goods_price{overflow:hidden;}
.wx-goods_price .dish-item-money{float:left;}
/*按钮*/
.flow-btn-min{background:#fff; border:1rpx solid #ccc; height:30px; line-height:30px; font-size:28rpx; width:200rpx; margin:0 auto; border-radius:5px; text-align: center; color:#777; margin-top:25rpx;}
 /*button*/
.button-common button{background:none;line-height:inherit;border-radius:0;border:0;font-size:30rpx}
.button-common button[disabled]:not([type]) {color:#fff;background-color:#ff495e;}
.button-common button::after{content:" ";width:0;height:0;border:none;-webkit-transform:scale(0);transform:scale(0);
-webkit-transform-origin:0 0;transform-origin:0 0;box-sizing:border-box;border-radius:0;}

/*底部浮动*/
.cart-tab-wrp{height: 100rpx; position: fixed; bottom: 0rpx; background: #fff;border-top:1rpx solid #ddd;
 display: flex; flex-direction:row; width: 100%; z-index: 10; color: black}
.cart-item-total{padding-left: 20rpx;}
.cart-item-icon-wrp{width: 110rpx; height: 90rpx; position: absolute; top:-4rpx;}
.cart-item-icon{width: 90rpx; height: 90rpx;}
.cart-item-total-num{display: inline-block; width: 36rpx; height: 36rpx; font-size: 22rpx; color: white; line-height: 36rpx; border-radius: 36rpx; text-align: center; position: absolute; right: 2rpx;}
.cart-item-total-price{font-size: 32rpx; color: #ff495e; text-align: center;}
.cart-item-total-price em{display: block}


.flow-list .activity-title .a-tag{background:#fba534;border-radius:20px;padding: 2rpx 12rpx;}
.flow-list .activity-title .a-cont{padding: 14rpx 0 0 20rpx;}
.flow-list .activity-title .a-cont .a-goods-tab{border-radius:20px;padding: 2rpx 12rpx;border:1px solid #ff495e;background:#ff495e; margin-left:10rpx;}
.flow-list .activity-title .a-jiantou{width:20rpx;}


/* .flow-list .header{line-height: inherit;height:inherit} */
.activity-goods{padding:12px;display: block}
.activity-goods .rideo-icon{display: none}
.activity-goods .imglist{width:180rpx;}
.activity-goods .imglist image{width:170rpx;height:170rpx}

.goods-attr{height: 50rpx;}
.cart-position-header{padding:6px 12px 10px 12px;position:fixed;top:0;left:0;right:0;z-index:100;border-bottom: 1px solid #eee;}
.activity-title{padding:8px 12px 5px 12px;border-top:1px solid #eee;
}
/*购物车列表*/
.cart-list{padding: 12px;}
.cart-list .cart-img{width:190rpx;margin-left: 15rpx;}
.cart-list .cart-img image{width:180rpx;height:180rpx}
.my-check{margin:25px 10px 0 0}

/*赠品*/
.gift-list-select{margin:0 20rpx 0 20rpx;}
.gift-list-select .cart-list{margin-bottom:10rpx;background:#FCF9F2;}

.flow-list custom-li{width:720rpx;margin:auto;}
.header{border-radius:15rpx 15rpx 0 0;margin-top:20rpx;}
.cart-list-data{border-radius: 0 0 15rpx 15rpx}
.flow-list .header .check, .check{width:40rpx;height:40rpx;
  position:relative;
  display: block;
  top:auto;
  margin-top:0rpx;
}